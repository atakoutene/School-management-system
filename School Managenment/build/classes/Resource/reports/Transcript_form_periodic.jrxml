				<textElement textAlignment="Justified" verticalAlignment="Middle">
					<font fontName="Times New Roman" pdfFontName="Times-Roman" isPdfEmbedded="true"/>
				</textElement>
				<text><![CDATA[Points = Number of Credits hours times Quality points]]></text>
			</staticText>
			<staticText>
				<reportElement positionType="Float" x="4" y="141" width="358" height="15" uuid="a2ff8e5a-076e-4e91-8275-9423bef3674a"/>
				<textElement textAlignment="Justified" verticalAlignment="Middle">
					<font fontName="Times New Roman" pdfFontName="Times-Roman" isPdfEmbedded="true"/>
				</textElement>
				<text><![CDATA[GPA (Grade Point Average)  = Total points divided by Total Credits hours]]></text>
			</staticText>
			<staticText>
				<reportElement positionType="Float" x="3" y="156" width="100" height="17" uuid="0d8910c2-c80a-418b-82e7-1a596c4b747a"/>
