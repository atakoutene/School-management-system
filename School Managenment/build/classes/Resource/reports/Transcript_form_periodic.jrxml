	<subDataset name="dataset1" uuid="8b4e6e95-6299-4c47-bfc6-20713ad8c10a">
		<parameter name="studentId" class="java.lang.Integer"/>
		<parameter name="idStartSemester" class="java.lang.Integer" isForPrompting="false"/>
		<parameter name="idEndSemester" class="java.lang.Integer" isForPrompting="false"/>
		<queryString>
			<![CDATA[select distinct
	c.code as coursecode,
	c.title as title, if(sem.name = 'Fall', 1, 2) as semesters,
	pc.nb_credits as creditshours,
	sc.score as myscore,
	sc.grade as marks,
	(case
      	 when sc.score = 'A' then 4*pc.nb_credits
      	 when sc.score = 'B' then 3*pc.nb_credits
      	 when sc.score = 'C' then 2*pc.nb_credits
      	 when sc.score = 'D' then 1*pc.nb_credits
       	else 0
       	end) as points,
	pe.name as studentname,
	dteStud.jour as journaiss,
	dteStud.mois as moisnaiss,
	dteStud.annee as anneenaiss,
	pe.placeofbirth as placeofbirth,
	if(pe.gender = 'M', 'Male','Female') as gender,
	p.title as major,
	s.studentId as refNum,
	if(s.level = 'Preparatory Semester' or s.level like 'Freshman %' or s.level like 'Sophomore %' or s.level like 'Junior %', 'Undergraduate','Postgraduate') as studenttype,
	if(sem.name = 'Spring', concat(sem.s_year - 1 ,'-',sem.s_year), concat(sem.s_year,'-',sem.s_year + 1)) as academicyear

from person pe, mydate dteStud, student_course sc, program_course pc, course c, course_offered co,
	program p, schoollevel sl, semester sem, student s

where s.id_person = pe.id and pe.id_date_birth = dteStud.id and
	s.id_program = p.id and  pc.id_program = p.id and
	pc.id_course = c.id and c.id = co.id_course and
	co.id = sc.id_course_offered and pc.id_semester = sem.id and
	sc.id_semester = sem.id and sc.id_student = s.id and s.id = $P{studentId} and sc.grade > 0
          and sem.id >= $P{idStartSemester} and sem.id <= $P{idEndSemester} order by academicyear, semesters ;]]>
		</queryString>
