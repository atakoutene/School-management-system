		<field name="moisnaiss" class="java.lang.Integer"/>
		<field name="anneenaiss" class="java.lang.Integer"/>
		<field name="placeofbirth" class="java.lang.String">
			<fieldDescription><![CDATA[]]></fieldDescription>
