	<queryString>
		<![CDATA[select distinct
	count(*) as numberofcourses,
	co.id as idCourseOffered,
	c.id as idCourse,
	c.code as coursecode,
	co.id as idCourseOffered,
	c.id as idCourse,
	c.code as coursecode,
	c.description as coursedescription,
	c.title as coursetitle,
	c.hours as coursehours,
	pc.passing_grade as passing_grade,
	pc.nb_credits as nbCredits,
	sc.grade as grade,
	sc.score as score,
	pers.name as studentName,
	pers.phonenumber as phone,
	pers.email as email,
	pers.placeofbirth as placebirth,
	dte.jour as daybirth,
	dte.mois as monthbirth,
	dte.annee as yearbirth,
	s.studentid as regNum,
	s.level as studentlevel,
	s.profilepicture as photo,
	p.title as studentprogram,
	sum(pc.nb_credits) as totalcredits


from student s, course_offered co, student_course sc, course c, program_course pc, program p, person pers, mydate dte
where co.id_course=c.id and
	pc.id_course = c.id and
	p.id = pc.id_program and
	co.id = sc.id_course_offered
	and s.id = sc.id_student and
	pers.id = s.id_person and
	pers.id_date_birth = dte.id and
	p.id = $P{id_program} and
	co.id_semester = pc.id_semester and
	co.course_year = $P{sem_year} and
	co.course_sem = $P{sem_name} and
	sc.id_student = $P{id_student};]]>
	</queryString>
