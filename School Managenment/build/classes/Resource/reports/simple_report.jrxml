	<subDataset name="dataset1" uuid="8b4e6e95-6299-4c47-bfc6-20713ad8c10a">
		<parameter name="id_program" class="java.lang.Integer">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<parameter name="sem_year" class="java.lang.Integer">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<parameter name="sem_name" class="java.lang.String">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<parameter name="id_level" class="java.lang.Integer">
			<defaultValueExpression><![CDATA[$P{id_program}]]></defaultValueExpression>
		</parameter>
		<parameter name="id_student" class="java.lang.Integer">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[select distinct
	c.code as coursecode,
	c.title as coursetitle,
	pc.nb_credits as nbCredits

from student s, course_offered co, student_course sc, course c, program_course pc, person pers
where co.id_course=c.id and
	pc.id_course = c.id and
	pc.id_program = $P{id_program} and
	co.id = sc.id_course_offered and
	s.id = sc.id_student and
	co.id_semester = pc.id_semester and
	co.course_year = $P{sem_year} and
	co.course_sem = $P{sem_name} and
	pers.id = s.id_person and
	sc.id_student = $P{id_student};]]>
		</queryString>
		<field name="coursecode" class="java.lang.String"/>
		<field name="coursetitle" class="java.lang.String"/>
		<field name="nbCredits" class="java.lang.Double"/>
	</subDataset>
	<parameter name="id_program" class="java.lang.Integer">
